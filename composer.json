{
  "name": "philipphermes/transfer-bundle",
  "type": "symfony-bundle",
  "description": "generated transfers from xml schema",
  "keywords": ["transfer", "dto", "generator"],
  "readme": "/README.md",
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Hermes"
    }
  ],
  "autoload": {
    "psr-4": {
      "PhilippHermes\\TransferBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhilippHermes\\TransferBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "symfony/framework-bundle": "7.2",
    "symfony/config": "^7.2",
    "symfony/console": "^7.2",
    "symfony/dependency-injection": "^7.2",
    "symfony/filesystem": "^7.2",
    "symfony/finder": "^7.2",
    "ext-simplexml": "*",
    "ext-libxml": "*",
    "ext-dom": "*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^12.1"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "phpstan": [
      "vendor/bin/phpstan analyse -c phpstan.neon"
    ],
    "test": [
      "XDEBUG_MODE=coverage vendor/bin/phpunit"
    ]
  }
}
