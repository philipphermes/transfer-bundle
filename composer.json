{
  "name": "philipphermes/transfer-bundle",
  "type": "symfony-bundle",
  "description": "generates transfers from xml",
  "keywords": ["transfer", "dto", "generator"],
  "readme": "/README.md",
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Hermes"
    }
  ],
  "autoload": {
    "psr-4": {
      "PhilippHermes\\TransferBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhilippHermes\\TransferBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.4",
    "ext-simplexml": "*",
    "nette/php-generator": "^v4.2",
    "symfony/config": "^8.0",
    "symfony/console": "^8.0",
    "symfony/dependency-injection": "^8.0",
    "symfony/filesystem": "^8.0",
    "symfony/finder": "^8.0",
    "symfony/framework-bundle": "^8.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^12.1",
    "zircote/swagger-php": "^5.7"
  },
  "suggest": {
    "symfony/security-bundle": "^8.0",
    "zircote/swagger-php": "^5.7"
  },
  "config": {
    "sort-packages": true
  }
}
